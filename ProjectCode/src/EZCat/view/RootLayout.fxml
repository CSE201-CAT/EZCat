<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="600.00" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="EZCat.view.RootLayoutController">
   <top>
      <ButtonBar prefHeight="40.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleViewProfileButton" text="View Profile" />
            <Button mnemonicParsing="false" onAction="#handlePeopleViewButton" text="People View" />
            <Button fx:id="bookmarksButton" mnemonicParsing="false" onAction="#handleBookmarksViewButton" text="Bookmarks" />
            <Button fx:id="newEditViewButton" disable="true" mnemonicParsing="false" onAction="#handleNewEditViewButton" text="New / Edit Requests" />
            <Button fx:id="deleteViewButton" disable="true" minWidth="69.0" mnemonicParsing="false" onAction="#handleDeleteViewButton" prefHeight="25.0" text="Delete Requests" />
        </buttons>
      </ButtonBar>
   </top>
</BorderPane>
